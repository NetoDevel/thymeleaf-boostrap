    xsi:schemaLocation="http://www.thymeleaf.org/extras/dialect
                        http://www.thymeleaf.org/xsd/thymeleaf-extras-dialect-2.1.xsd"
    prefix="bootstrap"
    namespace-uri="http://www.ppi.de/thymeleaf/bootstrap2"
    class="thymeleaf.bootstrap.dialect.BootstrapDialect">

